<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.mutablelogic.PostgresServerApp</string>
	<key>KeepAlive</key>
	<false/>
	<key>RunAtLoad</key>
	<false/>
	<key>ProgramArguments</key>
	<array>
	  <string>/Library/PreferencePanes/PostgreSQL.prefPane/Contents/MacOS/PostgresServerApp</string>
	  <string>-data</string>
	  <string>/Library/ApplicationSupport/PostgreSQL</string>
	</array>

	<!-- we use the _mysql user...similar!!! -->
	<key>UserName</key>
	<string>_mysql</string>
	<key>GroupName</key>
	<string>_mysql</string>
</dict>
</plist>
